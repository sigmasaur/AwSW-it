# game/chapter3.rpy:3416
translate italiantl _call_syscheck_18_8b444df8:

    # Ka exhausted dk "Ah, that can't be helped, then."
    Ka exhausted dk "Ah, niente da fare, allora."

# game/chapter3.rpy:3418
